{
  "keybindings": {
    "Normal": {
      "<q>": "Quit", // Quit the application
      "<Ctrl-d>": "Quit", // Another way to quit
      "<Ctrl-c>": "Quit", // Yet another way to quit
      "<Ctrl-z>": "Suspend", // Suspend the application
      "j": "Down", // Move down
      "k": "Up", // Move up
      "down": "Down", // Move down
      "up": "Up", // Move up
      "r": "GetRepos", // Refresh the list of repositories
      "enter": "Enter",
      "i": "Info",
      "h": "Help",
      "?": "Help",
    },
  }
}
